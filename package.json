{
  "name": "restaurant-booking-engine",
  "version": "1.0.0",
  "description": "Menu and Services Management for Restaurant Booking Engine",
  "main": "index.js",
  "scripts": {
    "start": "node -r module-alias/register dist/src/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "build": "tsc --build tsconfig.json",
    "knex": "knex --knexfile knexfile.ts",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ojigs/restaurant-booking-engine.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ojigs/restaurant-booking-engine/issues"
  },
  "homepage": "https://github.com/ojigs/restaurant-booking-engine#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/knex": "^0.15.2",
    "@types/node": "^25.0.8",
    "@types/pg": "^8.16.0",
    "eslint": "^9.39.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "_moduleAliases": {
    "@": "dist/src"
  }
}
